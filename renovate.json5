{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:best-practices',
    ':prConcurrentLimitNone',
    ':prHourlyLimitNone',
    ':rebaseStalePrs',
  ],
  packageRules: [
    {
      matchManagers: [
        'custom.regex',
      ],
      labels: [
        'regex',
      ],
    },
    {
      matchManagers: [
        'gomod',
      ],
      labels: [
        'gomod',
      ],
    },
  ],
  addLabels: [
    'renovate',
  ],
  vulnerabilityAlerts: {
    addLabels: [
      'security',
    ],
  },
}
